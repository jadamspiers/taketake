{
  "version": 1,
  "paths": {
    "/matchmaker": {
      "name": "/matchmaker",
      "lambdaFunction": "matchmakerHandler",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/leaderboard": {
      "name": "/leaderboard",
      "lambdaFunction": "leaderboard",
      "permissions": {
        "setting": "open"
      }
    },
    "/joinTournament": {
      "name": "/joinTournament",
      "lambdaFunction": "joinTournamentHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}